{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "react",
    "stage-0",
    "es2015",
    "es2016",
    "es2017",
    "env"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    [
        "react-css-modules",
        {
        "generateScopedName": "[name]_[local]",
        "filetypes": {
            ".scss": {
            "syntax": "postcss-scss"
            }
        }
        }
    ],
    [
        "import",
        {
          "libraryName": "antd-mobile",
          "style": "css"
        }
    ]
  ]
}
